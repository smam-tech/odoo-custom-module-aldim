<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="aldim_imgbb_res_config_settings_form" model="ir.ui.view">
        <field name="name">aldim.imgbb.res_config_settings_form</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='recaptcha']" position="after">
            <div class="col-12 col-lg-6 o_setting_box" id="imgbb">
                <span class="o_form_label">Imgbb Integration</span>
                <div class="text-muted">
                    Key Imgbb
                </div>
                <field name="key_imgbb"/>
                <div class="text-muted">
                    Time Image stay at imgbb, in second
                </div>
                <field name="time_imgbb"/>
            </div>
            </xpath>
        </field>
    </record>
</odoo>